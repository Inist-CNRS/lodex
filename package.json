        "node": ">= 16"
    },
    "name": "lodex",
    "version": "15.9.11",
    "description": "",
    "main": "src/api/index.ts",
    "browser": {
        "config": false
    },
    "scripts": {
        "start": "concurrently \"npm:production:*\"",
        "production:api": "tsx --max-http-header-size=32768 src/api",
        "production:workers": "node --require @babel/register workers",
        "build": "npx vite build --config ./vite.config.js",
        "development:api": "tsx watch --max-http-header-size=32768 src/api",
        "development:app": "npx vite --port=8080 --host=0.0.0.0",
        "postversion": "git push && git push --tags",
        "type-check": "tsc --noEmit",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
        "lint:test": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
        "clean": "concurrently \"npm:clean:*\"",
        "clean:modules": "modclean -Pr -n ./modclean.config.js:*",
        "clean:workers": "modclean -Pr -n ./modclean.config.js:* -D workers",
        "clean:packages": "modclean -Pr -n ./modclean.config.js:* -D packages",
        "test:unit": "NODE_OPTIONS='--max_old_space_size=8192' ./node_modules/jest/bin/jest.js --force-exit --colors --maxWorkers=1",
        "test:unit:watch": "NODE_OPTIONS='--max_old_space_size=8192' ./node_modules/jest/bin/jest.js --watchAll --colors",
        "test:e2e:phase:one": "cypress run --browser chrome --config-file cypress.config.js  --spec \"cypress/e2e/phase_1/*.cy.js\"",
        "test:e2e:phase:two": "cypress run --browser chrome --config-file cypress.config.js  --spec \"cypress/e2e/phase_2/*.cy.js\"",
        "test:e2e:phase:three": "cypress run --browser chrome --config-file cypress.config.js  --spec \"cypress/e2e/phase_3/*.cy.js\"",
        "test:e2e:phase:four": "cypress run --browser chrome --config-file cypress.config.js --spec \"cypress/e2e/phase_4/annotation.cy.js\"",
        "test:e2e:all": "cypress run --browser chrome",
        "insertversion:readme": "sed -i \"s#\\(lodex:\\)\\([\\.a-z0-9]\\+\\)#\\1${npm_package_version}#g\" README.md",
        "insertversion:makefile": "sed -i \"s#\\(lodex:\\)\\([\\.a-z0-9]\\+\\)#\\1${npm_package_version}#g\" Makefile",
        "insertversion:docker": "sed -i \"s#\\(lodex:\\)\\([\\.a-z0-9]\\+\\)#\\1${npm_package_version}#g\" docker-compose*.yml",
        "version": "npm run insertversion:readme && npm run insertversion:makefile && npm run insertversion:docker && git add README.md Makefile",
        "postinstall": "cd ./packages/ezsLodex/ && npm install --production --install-strategy=nested --legacy-peer-deps && cd ../transformers/ && npm install --production --install-strategy=nested --legacy-peer-deps"
    },
    "nodemonConfig": {
        "ignore": [
            "*.spec.js"
        ],
        "delay": 2500
    },
