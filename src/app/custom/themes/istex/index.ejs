    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0"/>
    <link rel="apple-touch-icon" sizes="180x180" href="<%= theme.base.href %>/themes/istex/apple-touch-icon.png"/>
