    <link rel="icon" type="image/png" sizes="16x16" href="<%= theme.current.href %>/favicon-16x16.png"/>
    <link rel="alternate" type="application/atom+xml" href="/api/export/atom"/>
    <link rel="manifest" href="<%= theme.current.href %>/manifest.json"/>
    <link rel="mask-icon" href="<%= theme.current.href %>/safari-pinned-tab.svg" color="#5bbad5"/>
    <link rel="stylesheet" href="<%= theme.current.href %>/css/style.css"/>
    <link rel="stylesheet" href="<%= theme.current.href %>/css/istex.css"/>
    <% if (custom.sousTheme == 'corpus') { %>
    <link rel="stylesheet" href="<%= theme.current.href %>/css/corpus.css" />
    <% } %>
    <% if (custom.sousTheme == 'termino') { %>
